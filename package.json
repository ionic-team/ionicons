{
  "name": "ionicons",
  "version": "4.0.0",
  "description": "Premium icons for Ionic.",
  "files": [
    "dist/"
  ],
  "main": "dist/ionicons.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "build": "npm run build.icon && npm run generate && npm run png && npm run build.data && npm run build.css && npm run build.zip && npm run build.docs && npm run icon-copy",
    "build.icon": "stencil build --prod",
    "build.docs": "stencil build --config src/docs/stencil.config.js",
    "build.data": "node scripts/data.js",
    "build.css": "node scripts/build-css.js",
    "build.zip": "node scripts/build-zip.js",
    "icon-copy": "node scripts/copy-svg-to-dist.js",
    "mode.comparison": "node scripts/mode-comparison.js",
    "dev": "npm run build && sd concurrent \"stencil build --dev --config src/docs/stencil.config.js --watch\" \"stencil-dev-server\" ",
    "generate": "python2 scripts/generate.py",
    "png": "node scripts/png.js",
    "release": "npm run build && np --any-branch"
  },
  "devDependencies": {
    "@ionic/jpml": "latest",
    "@stencil/core": "^0.8.2",
    "@stencil/dev-server": "latest",
    "@stencil/router": "^0.1.6",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "fs-extra": "4.0.2",
    "highlight.js": "^9.12.0",
    "node-sass": "4.9.0",
    "np": "2.20.1",
    "svg2png": "4.1.1",
    "svgo": "1.0.5"
  },
  "keywords": [
    "ionicons",
    "ionic",
    "icon",
    "svg",
    "font",
    "mobile",
    "web component",
    "component",
    "custom element"
  ],
  "homepage": "http://ionicons.com/",
  "author": {
    "name": "Ben Sperry",
    "web": "https://twitter.com/benjsperry"
  },
  "contributors": [
    {
      "name": "Adam Bradley",
      "web": "http://twitter.com/adamdbradley"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionicons.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/ionicons/issues"
  },
  "license": "MIT",
  "dependencies": {
    "archiver": "^2.1.1"
  }
}
